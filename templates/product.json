    "fed4c0f7-a6de-4294-aaeb-3a5e67408483": {
      "type": "pdp-two-columns-richtext",
      "blocks": {
        "992b6196-f72e-46cf-99ec-9a3706c8e816": {
          "type": "left_column",
          "settings": {
            "icon": "shopify:\/\/shop_images\/small_logo_ab97bda9-9945-47ad-8998-f07223e8f88e.png",
            "content": "<ul><li>Includes beneficial vitamins & supplements to support your everyday health<\/li><li>Hydrating foam promotes a balanced oral microbiome<\/li><li>Contains botanical extracts and natural ingredients, including antioxidants and nHA<\/li><li>Plastic-free, mess-free, travel-friendly and designed for your bathroom sink<\/li><\/ul>"
          }
        },
        "cdfcf401-a321-4cfc-bbbc-654b6e7025a0": {
          "type": "right_column",
          "settings": {
            "heading": "Standard toothpaste",
            "content": "<ul><li>No extra health benefit aside from standard oral care<\/li><li>Contains SLS, which dries your mouth and can lead to bad breath<\/li><li>Uses fillers, harsh cleaning agents, and other potentially toxic chemicals<\/li><li>Gunky paste in plastic tubes that can’t be recycled<\/li><\/ul>"
          }
        }
      },
      "block_order": [
        "992b6196-f72e-46cf-99ec-9a3706c8e816",
        "cdfcf401-a321-4cfc-bbbc-654b6e7025a0"
