      "type": "info-icons-with-text",
      "blocks": {
        "32e3ada9-5cde-4e18-8462-8c37c6b975b9": {
          "type": "text",
          "settings": {
            "icon": "shopify:\/\/shop_images\/slider_Icon_1.svg",
            "title": "Brightly whitens teeth",
            "text": "<p>Strong enough to get out stains. Gentle enough to not strip enamel. <\/p>",
            "name": ""
          }
        },
        "5b38250a-3ad7-4485-a300-fa9e3bb24df7": {
          "type": "text",
