}

function currentSlide_1(n) {
  showSlide_1(slideIndex_1 = n);
}

function showSlide_1(n) {
  var i;
//   var j;
  var slides_1 = document.getElementsByClassName("mySlide_1");
  var dots_1 = document.getElementsByClassName("link1");
//   var removeClass = document.getElementsByClassName("myAnchor");
  
  if (n > slides_1.length) {slideIndex_1 = 1}
  if (n < 1) {slideIndex_1 = slides_1.length}
  for (i = 0; i < slides_1.length; i++) {
      slides_1[i].style.display = "none";
  }
  for (i = 0; i < dots_1.length; i++) {
      dots_1[i].className = dots_1[i].className.replace(" active", "");
  }
  
//   for (j = 0; j < removeClass.length; j++) {
//       removeClass[j].classList.remove('class_1');	
//   }
  slides_1[slideIndex_1 - 1].style.display = "block";
  dots_1[slideIndex_1 - 1].className += " active";
  
}
  
  
