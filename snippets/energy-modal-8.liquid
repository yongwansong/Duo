    function energy_showSlide_8(n) {
        var i;
        var energy_slides_8 = document.getElementsByClassName("energy_Slider_8");
        var energy_dots_8 = document.getElementsByClassName("energy_link8");

        if (n > energy_slides_8.length) {
            energy_slideIndex_8 = 1
        }
        if (n < 1) {
            energy_slideIndex_8 = energy_slides_8.length
        }
        for (i = 0; i < energy_slides_8.length; i++) {
            energy_slides_8[i].style.display = "none";
        }
        for (i = 0; i < energy_dots_8.length; i++) {
            energy_dots_8[i].className = energy_dots_8[i].className.replace(" active", "");
        }
        energy_slides_8[energy_slideIndex_8 - 1].style.display = "block";
        energy_dots_8[energy_slideIndex_8 - 1].className += " active";

    }
</script>

