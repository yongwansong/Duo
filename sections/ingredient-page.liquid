<script>
      var modalparent_m = document.getElementsByClassName('modal_mobile');
      var modal_btn_mobile = document.getElementsByClassName('myBtn_multi mobile__link');
      var span_close_mobile = document.getElementsByClassName('close_multi_m');

  
      function setDataIndexM() {
        for (i = 0; i < modal_btn_mobile.length; i++) {
          modal_btn_mobile[i].setAttribute('data-index', i);
