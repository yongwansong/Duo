<script>
      var modalparent = document.getElementsByClassName('modal_multi');

      var modal_btn_multi = document.getElementsByClassName('myBtn_multi ingredients_link btn-link');
      var btn_base_count = document.getElementById('base').getElementsByClassName('myBtn_multi ingredients_link btn-link').length;
      var btn_immunity_count = document.getElementById('immunity').getElementsByClassName('myBtn_multi ingredients_link btn-link').length;
      var btn_energy_count = document.getElementById('energy').getElementsByClassName('myBtn_multi ingredients_link btn-link').length;
      var btn_sleep_count = document.getElementById('sleep').getElementsByClassName('myBtn_multi ingredients_link btn-link').length;

      var span_close_multi = document.getElementsByClassName('close_multi');

      
      var modalbase = document.getElementsByClassName('baseline_content');
      var modalimm = document.getElementsByClassName('immunity_content');
      var modalenergy = document.getElementsByClassName('energy_content');
      var modalsleep = document.getElementsByClassName('sleep_content');

  
      function setDataIndex() {
        for (i = 0; i < modal_btn_multi.length; i++) {
          modal_btn_multi[i].setAttribute('data-index', i);
        }

        for (i = 0; i < modalparent.length; i++) {
          modalparent[i].setAttribute('data-index', i);
          span_close_multi[i].setAttribute('data-index', i);
        }
      }
      for (i = 0; i < modal_btn_multi.length; i++) {

        
        modal_btn_multi[i].onclick = function () {
          var ElementIndex = this.getAttribute('data-index');
          if (ElementIndex < btn_base_count) {
            modalparent[0].style.display = 'block';
          } else if (ElementIndex < btn_base_count+btn_immunity_count) {
            modalparent[1].style.display = 'block';

          } else if (ElementIndex < (btn_base_count+btn_immunity_count+btn_energy_count)) {
            modalparent[2].style.display = 'block';

          } else if (ElementIndex < (btn_base_count+btn_immunity_count+btn_energy_count + btn_sleep_count)) {
                      modalparent[3].style.display = 'block';
          }


        };

        // When the user clicks on <span> (x), close the modal
      }
      
      for (i = 0; i < span_close_multi.length; i++) {

        span_close_multi[i].onclick = function () {
            var ElementIndex = this.getAttribute('data-index');
            modalparent[ElementIndex].style.display = 'none';
        };
      }


      var modal = document.getElementById('myModal');
      var btn = document.getElementById('myBtn');
      if (btn !== null) {
        btn.onclick = function () {
          modal.style.display = 'block';
        };
      }
      if (modal !== null) {
        var span = modal.getElementsByClassName('close')[0]; // Modified by dsones uk
        span.onclick = function () {
          modal.style.display = 'none';
        };
      }
    </script>

