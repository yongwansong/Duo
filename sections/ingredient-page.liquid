          span_close_mobile[i].setAttribute('data-index', i);
        }
      }

      for (i = 0; i < modal_btn_mobile.length; i++) {
        modal_btn_mobile[i].onclick = function () {
          var ElementIndex = this.getAttribute('data-index');
          modalparent_m[ElementIndex].style.display = 'block';
        };

        // When the user clicks on <span> (x), close the modal
        span_close_mobile[i].onclick = function () {
          var ElementIndex = this.getAttribute('data-index');
          modalparent_m[ElementIndex].style.display = 'none';
        };
      }


      var modal_m = document.getElementById('myModal');
