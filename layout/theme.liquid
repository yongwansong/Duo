  </script>
  
  </head>
  <body class="gradient template--{{ template.name }} {% unless template contains 'index' %}sub-bg{% endunless %} {% if template contains 'index' %} home_page{%endif%} bg-main-color {% if collection.handle == 'all' %} collection_all{%endif%} {% if collection.handle == 'products' %} collection_all{%endif%} {% if template contains 'subscriptions-landing' %}subscriptions-landing-page {%endif%}{% if template.name == 'product' %} template-product--{{ product.handle }}{% endif %}">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ 'accessibility.skip_to_text' | t }}
    </a>
    {%- if settings.cart_type == 'drawer' and template.name != 'cart' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}

    {% section 'announcement-bar' %}
    {% section 'header' %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {%- if request.path contains 'loop_subscriptions/bundle' -%}
        {% render 'loop_bundle_loader' %}
      {%- endif -%}

      {{ content_for_layout }}
    </main>

    {% section 'footer' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
    </ul>

    <script>
      window.shopUrl = '{{ shop.url }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}',
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`,
      };

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      };

      window.accessibilityStrings = {
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
      };
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    <!-- Link text with Image -->
    <script>
      $(document).ready(function () {
        $('.accordion_heading').click(function () {
          $(this).next().slideToggle(500);
          $('.accordion_text').not($(this).next()).slideUp(500);
        });
      });
    </script>
    <!-- <div data-rebuy-id="36899"></div> -->
    <!--
      <div data-rebuy-id="36716"></div>
      <div data-rebuy-id="36572"></div>
    -->
    {% comment %} {% include 'smart-cart-template' %} {% endcomment %}
   {% include 'smile-initializer' %}

