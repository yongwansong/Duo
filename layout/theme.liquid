    <script
      type="text/javascript"
      id="loop-subscription-script"
      src="{{ 'loop-widget.js' | asset_url }}"
      defer
    ></script>
    <style class="loop-style">
      {% render 'loop-widget.css' %}
    </style>
    {%- if request.path contains 'loop_subscriptions/bundle' -%}
      {{ 'loop-bundle.css' | asset_url | stylesheet_tag }}
      <script>
        jQuery(document).ready(function ($) {
          var checkExist = setInterval(function () {
            if (
              $('.loop .loop-products-available .loop-products-align .loop-card .loop-bundle-product-card-title').length
            ) {
              $('.loop .loop-products-available .loop-products-align .loop-card .loop-bundle-product-card-title').each(
                function () {
                  var text = $(this).text();
                  var index = text.indexOf('-');
                  if (index > 0) {
                    $(this).text(text.substring(0, index));
                  }
                }
              );
              clearInterval(checkExist);
            }
          }, 100);
          setTimeout(function () {
            clearInterval(checkExist);
          }, 10000);
        });
      </script>
    {%- endif -%}
